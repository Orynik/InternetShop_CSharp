@using WebUI.Models
@model BooksListViewModel

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<div>
    @foreach (var book in Model.Books)
    {
        
      @Html.Partial("_BookSummary",book)
    }
</div>
<div class="btn-group pull-right">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("List", new { Page = x }))
</div>